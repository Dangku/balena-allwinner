setenv resin_kernel_load_addr ${kernel_addr_r}
run resin_set_kernel_root
env set bootargs "${bootargs} console=${console} console=tty1 ${resin_kernel_root} rootfstype=ext4 rootwait panic=10 ${extra}"
echo "load dtb and kernel from ${resin_dev_type} ${resin_dev_index}:${resin_boot_part}"
fatload ${resin_dev_type} ${resin_dev_index}:${resin_boot_part} ${fdt_addr_r} dtb/${fdtfile}
fatload ${resin_dev_type} ${resin_dev_index}:${resin_boot_part} ${resin_kernel_load_addr} @@KERNEL_IMAGETYPE@@
@@KERNEL_BOOTCMD@@ ${resin_kernel_load_addr} - ${fdt_addr_r}
